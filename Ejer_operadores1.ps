Algoritmo operadores
	Escribir "Escribir el primer numero"
	leer a
	Escribir "escribir el segundo numero"
	leer b

	si a>b Entonces
		Escribir "a es mayor que b"
		suma=a+b
	sino 	
		Escribir "b es mayor que a"
		producto=a*b
	FinSi
	Escribir "El valor de la suma es: " suma;
	Escribir "El valor del producto es" producto;
	
	
FinAlgoritmo
